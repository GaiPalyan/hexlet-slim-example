    "minimum-stability": "stable",
    "require": {
        "slim/slim": "^4.8",
        "slim/psr7": "^1.4",
        "slim/http": "^1.2",
        "fzaninotto/faker": "*",
        "tightenco/collect": "^8.34",
        "guzzlehttp/guzzle": "*",
        "slim/php-view": "^3.1",
        "php-di/php-di": "^6.3",
        "symfony/string": "^5.3",
        "squizlabs/php_codesniffer": "^3.6",
        "slim/flash": "^0.4.0"
    },
    "require-dev": {
        "heroku/heroku-buildpack-php" : "dev-master"
    },
    "scripts": {
        "phpcs": "phpcs --standard=PSR12 src public"
